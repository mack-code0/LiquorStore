<!-- Script for slidin in account-section navbar -->
<script>
	var first = document.querySelector(".first-sect")
	var body = document.querySelector(".no-dark-body")
	var show = document.querySelector(".show-first-sect")
	var num = 0;
	show.addEventListener("click", ()=>{
		if(num == 0){
			body.classList.replace("no-dark-body", "dark-body")
			show.classList.add("show-first-sect-move")
			first.classList.replace("first-sect", "first-sect-show")
			num = 1;
		}else{
			body.classList.replace("dark-body", "no-dark-body")
			show.classList.remove("show-first-sect-move")
			first.classList.replace("first-sect-show", "first-sect")
			num = 0;
		}
	})
</script>
